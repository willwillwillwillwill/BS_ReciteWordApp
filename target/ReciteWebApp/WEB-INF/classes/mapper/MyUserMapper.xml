<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="www.george.com.mapper.MyUserMapper">
    <select id="getUser" resultType="www.george.com.dao.MyUser">
        SELECT *
        FROM user
        WHERE id = #{id}
    </select>

</mapper>